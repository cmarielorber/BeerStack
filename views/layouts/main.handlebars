<html lang='en'>

  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>BeerStack</title>
    <link
      rel='stylesheet'
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css'
      integrity='sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi'
      crossorigin='anonymous'
    />
    <link href="https://getbootstrap.com/docs/5.2/assets/css/docs.css" rel="stylesheet">
    <link rel='stylesheet' href='../css/style.css' />
    {{!-- <link rel='stylesheet' href='../css/custom.css' /> --}}
    <script
      src='https://kit.fontawesome.com/93df6eac37.js'
      crossorigin='anonymous'
    ></script>
    <link rel='preconnect' href='https://fonts.googleapis.com' />
    <link rel='preconnect' href='https://fonts.gstatic.com' crossorigin='' />
    <link
      href='https://fonts.googleapis.com/css2?family=Special+Elite&display=swap'
      rel='stylesheet'
    />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://getbootstrap.com/docs/5.2/assets/css/docs.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
  </head>

  <main>
    <header>{{> nav-bar login_status=login_status }}</header>
    <body>
      <div class="mainAside row">
          <div class="sideAside col-lg-2">
              <ul class="nav flex-column text-center">
              <li class="nav-itemmain">
                <a class="nav-linkmain" href="/">Home</a>
              </li>
              <li class="nav-itemmain">
                <a class="nav-linkmain" href="/dashboard">Dashboard</a>
              </li>
              <li class="nav-itemmain">
                <a class="nav-linkmain" href="/beerpost">Brewclopedia</a>
              </li>
              <li class="nav-itemmain">
                <a class="nav-linkmain" href="/socalbrew">So Cal Brewery</a>
              </li>
              {{#if login_status}}
              <li class="nav-itemmain" id='logout-link'>
                <a
                style='text-decoration: none; color: var(--brew);'
                id='logout-link'
                >Logout
                </a>
              </li>
              {{/if}}        
            </ul>
          </div>
          <div class="col-lg-10">
            <div class="mainhbsBody">
              {{{body}}}
            </div>
          </div>
        </div>
      </div>

      <script src='https://code.jquery.com/jquery-3.5.1.min.js'></script>
      <script
        src='https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js'
        integrity='sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3'
        crossorigin='anonymous'
      ></script>
      <script src='/js/script.js'></script>
      <script src='/js/logout.js'></script>


    </body>
    </main>
    
  </html>

